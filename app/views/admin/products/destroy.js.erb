<% if flash[:success].present? %>
  $('#flash')
    .html('<div class="alert alert-success"><%= flash[:success] %></div>')
<% end %>
<% if flash[:danger].present? %>
  $('#flash')
    .html('<div class="alert alert-danger"><%= flash[:danger] %></div>')
<% end %>
$('.products')
  .html('<%= j render partial: "list_product", locals: {products: @products} %>')
